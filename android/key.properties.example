# Copia este archivo como key.properties y rellena los valores reales.
# key.properties no se sube a Git (está en .gitignore).

storePassword=TU_CONTRASEÑA_KEYSTORE
keyPassword=TU_CONTRASEÑA_KEY
keyAlias=upload
